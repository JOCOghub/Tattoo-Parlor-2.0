
<%=form_for @appointment do |f|%>
  <%=f.label :time%>
  <%=f.date_field :appointment_time %>
   
  <% if !@artist %>
    <br>
    <%=f.fields_for :artist do |i|%>
      <%=i.label :Artist %>
      <%=i.text_field :name %>
    <%end%>
  <%end%>
  <br>
 <%=f.submit "Book Appointment"%>

<%end%>
